<div>
    <h1 class="card z-depth-3 center-align">Read the Latest from 'The New york Times'</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<a href='/scrape' method="GET">Scrape</a>

<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each articles}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            {{this.summary}}
            {{!-- goes to article.handlebars --}}
            <a href="/articles/{{this._id}}" class="waves-effect waves-light btn pink lighten-1" id="btnSignUp">Read
                Article</a>
        </div>
        {{/each}}
    </div>
</div>